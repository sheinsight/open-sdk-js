{
  "name": "@sheinsight/open-sdk-js",
  "version": "1.0.0",
  "description": "A TypeScript SDK for generating Shein API signatures",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"demo/**/*.{js,ts}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"demo/**/*.{js,ts}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build && pnpm run test && pnpm run lint",
    "demo": "node demo/simple-example.js",
    "demo:typescript": "npx ts-node demo/typescript-example.ts",
    "clean": "rimraf dist coverage",
    "prepack": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "shein",
    "signature",
    "hmac",
    "sha256",
    "api",
    "sdk",
    "typescript",
    "authentication",
    "crypto",
    "nodejs"
  ],
  "author": "Limbor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sheinsight/open-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/sheinsight/open-sdk-js/issues"
  },
  "homepage": "https://github.com/sheinsight/open-sdk-js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.14",
    "@types/js-base64": "3.3.1",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.1",
    "ts-jest": "^29.4.1",
    "tslib": "2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@types/crypto-js": "4.2.2",
    "axios": "1.12.2",
    "crypto-js": "4.2.0",
    "js-base64": "3.7.8"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@8.0.0"
}
