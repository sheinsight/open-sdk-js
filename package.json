{
  "name": "@sheinsight/open-sdk-js",
  "version": "1.0.1",
  "description": "A TypeScript SDK for Shein API Tools",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./esm/*": {
      "types": "./lib/esm/*.d.ts",
      "import": "./lib/esm/*.js",
      "default": "./lib/esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"demo/**/*.{js,ts}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"demo/**/*.{js,ts}\"",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run typecheck && pnpm run lint && pnpm run format:check",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run check && pnpm run test && pnpm run build",
    "demo": "node demo/simple-example.js",
    "demo:typescript": "npx ts-node demo/typescript-example.ts",
    "clean": "rimraf lib coverage",
    "prepack": "pnpm run clean && pnpm run build",
    "size": "find lib/esm -name \"*.js\" -exec wc -c {} + | tail -1"
  },
  "keywords": [
    "shein",
    "signature",
    "hmac",
    "sha256",
    "api",
    "sdk",
    "typescript",
    "authentication",
    "crypto",
    "nodejs",
    "tree-shaking",
    "esm",
    "commonjs",
    "encryption",
    "decryption",
    "aes",
    "open-api",
    "rest-api"
  ],
  "author": "Limbor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sheinsight/open-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/sheinsight/open-sdk-js/issues"
  },
  "homepage": "https://github.com/sheinsight/open-sdk-js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/crypto-js": "4.2.2",
    "@types/jest": "^29.5.14",
    "@types/js-base64": "3.3.1",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.1",
    "ts-jest": "^29.4.1",
    "tslib": "2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "axios": "1.12.2",
    "crypto-js": "4.2.0",
    "js-base64": "3.7.8"
  },
  "peerDependencies": {
    "@types/crypto-js": "^4.2.0"
  },
  "peerDependenciesMeta": {
    "@types/crypto-js": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@8.0.0"
}
